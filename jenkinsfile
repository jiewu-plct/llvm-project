node("riscv64_runner1"){
    stage('*** Cleanup***') {
        cleanWs()
    }
    stage('*** Check out SCM ***') {
        checkout scm
    } 
    stage('*** Compilation Phase ***') { 
        sh '''#!/bin/bash
            mkdir build
            cd build
            cmake -DLLVM_ENABLE_PROJECTS="clang" -DCMAKE_BUILD_TYPE="Release" -G Ninja ../llvm
            ninja
         '''
    }
    stage (' *** Test ***'){
        sh '''#!/bin/bash
            cd build
            bin/clang -v
            ninja check
         '''
    }
}
